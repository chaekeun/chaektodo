m\";\nimport Register from \"./components/register\";\nimport Tasks from \"./components/tasks\";\nimport { React, useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport Write from \"./components/write\";\nimport NotFound from \"./components/notFound\";\nimport Fail from \"./components/fail\";\nimport Home from \"./components/home\";\n\nfunction App() {\n  let [tasks, setTasks] = useState([]);\n  let [timer, setTimer] = useState(\"\");\n\n  function Timer() {\n    const currentTimer = () => {\n      let today = new Date();\n      today = today.toLocaleString(\"ko-kr\");\n      setTimer(today);\n    };\n\n    const startTimer = () => {\n      setInterval(currentTimer, 1000);\n    };\n    startTimer();\n  }\n\n  return (\n    <div className=\"App\">\n      <Routes>\n        <Route path=\"/\" element={<Home></Home>}></Route>\n        <Route path=\"/register\" element={<Register></Register>}></Route>\n        <Route path=\"/fail\" element={<Fail></Fail>}></Route>\n\n        <Route\n          path=\"/tasks/:user\"\n          element={\n            <div>\n              <Timer /> <h4>{timer}</h4>\n              <Tasks tasks={tasks} setTasks={setTasks}></Tasks>\n            </div>\n          }\n        ></Route>\n        <Route path=\"/write\" element={<Write timer={timer}></Write>}></Route>\n\n        <Route path=\"/*\" element={<NotFound></NotFound>}></Route>\n      </Routes>\n    </div>\n  );\n}\n\nexport default App;\n","/mnt/c/Users/chaen/Desktop/김책은/web 공부/TodoList/todoapp/myreact/src/components/register.js",[],[],"/mnt/c/Users/chaen/Desktop/김책은/web 공부/TodoList/todoapp/myreact/src/components/write.js",["63","64"],[],"import { useEffect, useState } from \"react\";\r\nimport { useNavigate } from \"react-router-dom\";\r\n\r\nfunction Write(timer) {\r\n  let navigate = useNavigate();\r\n  // console.log(timer.timer);\r\n  return (\r\n    <div>\r\n      <h4 className=\"container mt-4\">\r\n        <strong>Add your Task!</strong>\r\n      </h4>\r\n\r\n      <div className=\"container mt-3\">\r\n        <form action=\"/api/add\" method=\"POST\">\r\n          <div class=\"form-group\">\r\n            <label>title</label>\r\n            <input type=\"text\" className=\"form-control\" name=\"title\" />\r\n            <label>content</label>\r\n            <input type=\"text\" className=\"form-control\" name=\"content\" />\r\n            <input\r\n              type=\"text\"\r\n              value={timer.timer}\r\n              style={{ display: \"none\" }}\r\n              name=\"created_at\"\r\n            />\r\n          </div>\r\n\r\n          <button type=\"submit\" className=\"btn btn-outline-secondary\">\r\n            Submit\r\n          </button>\r\n        </form>{\" \"}\r\n        <button\r\n          className=\"btn btn-outline-secondary\"\r\n          onClick={() => {\r\n            navigate(-1);\r\n          }}\r\n        >\r\n          Cancle\r\n        </button>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default Write;\r\n","/mnt/c/Users/chaen/Desktop/김책은/web 공부/TodoList/todoapp/myreact/src/components/notFound.js",[],[],"/mnt/c/Users/chaen/Desktop/김책은/web 공부/TodoList/todoapp/myreact/src/components/fail.js",["65","66"],[],"/mnt/c/Users/chaen/Desktop/김책은/web 공부/TodoList/todoapp/myreact/src/components/tasks.js",["67","68","69","70","71","72"],[],"import { React, useEffect, useState } from \"react\";\r\nimport axios from \"axios\";\r\nimport { Link, Navigate, useParams } from \"react-router-dom\";\r\nimport DeleteModal from \"./deleteModal\";\r\nimport { Modal, Nav } from \"react-bootstrap\";\r\nimport EditModal from \"./editModal\";\r\n\r\nfunction Tasks({ tasks, setTasks }) {\r\n  let [num, setNum] = useState(0);\r\n  let [deleteModal, setDeleteModal] = useState(false);\r\n  let [editModal, setEditModal] = useState(false);\r\n\r\n  let { user } = useParams();\r\n\r\n  useEffect(() => {\r\n    fetchData();\r\n  }, []);\r\n\r\n  const fetchData = () => {\r\n    axios\r\n      .get(`/api/tasks/${user}`)\r\n      .then((res) => {\r\n        let copy = [...res.data];\r\n        setTasks(copy);\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n  };\r\n\r\n  const completeTask = async (taskId, status) => {\r\n    try {\r\n      const stringStatus = String(status);\r\n      await axios.post(`/api/complete/${taskId}/${stringStatus}`);\r\n      fetchData();\r\n      console.log(\"Task completed successfully\");\r\n    } catch (err) {\r\n      console.error(\"Error completing task:\", err);\r\n    }\r\n  };\r\n\r\n  const deleteTask = async (taskId) => {\r\n    try {\r\n      await axios.delete(`/api/delete/${taskId}`);\r\n      fetchData();\r\n      console.log(\"Task deleted successfully\");\r\n    } catch (err) {\r\n      console.error(\"Error deleting task: \", err);\r\n    }\r\n  };\r\n\r\n  const signOut = async () => {\r\n    try {\r\n      await axios.get(\"/api/signout\");\r\n      console.log(\"Sign out succesfully\");\r\n    } catch (err) {\r\n      console.error(\"Error:\", err);\r\n    }\r\n  };\r\n\r\n  const deleteCancle = () => {\r\n    setDeleteModal(false);\r\n  };\r\n\r\n  const deleteConfirm = (taskId) => {\r\n    deleteTask(taskId);\r\n    setDeleteModal(false);\r\n  };\r\n\r\n  const editCancle = () => {\r\n    setEditModal(false);\r\n  };\r\n\r\n  const editConfirm = () => {\r\n    setEditModal(false);\r\n  };\r\n\r\n  {\r\n    /* {editModal && (\r\n        <div className=\"Listbox edit\">\r\n          <EditModal\r\n            taskId={tasks[num]._id}\r\n            title={tasks[num].title}\r\n            content={tasks[num].content}\r\n            onClickCancle={editCancle}\r\n          ></EditModal>\r\n        </div>\r\n      )} */\r\n  }\r\n\r\n  return (\r\n    <div>\r\n      <Link to=\"/write\">Add the task</Link>\r\n      <br></br>\r\n      <a href=\"/api/signout\">signOut</a>\r\n\r\n      <div className=\"ListBox\">\r\n        {tasks.map((a, i) => {\r\n          return (\r\n            <div className={\"Tasklist\"} key={i}>\r\n              <div className=\"left-items\">\r\n                <div className={a.completed ? \"strikeThrough\" : \"\"}>\r\n                  <h4>{a.title}</h4>\r\n                  <p> {a.content}</p>\r\n                  <div style={{ fontSize: \"12px\" }}> {a.created_at}</div>\r\n                </div>\r\n              </div>{\" \"}\r\n              <div className=\"right-items\">\r\n                <button\r\n                  variant=\"dark\"\r\n                  onClick={() => {\r\n                    setNum(i);\r\n                    setEditModal(true);\r\n                    console.log(editModal + \" edit button\");\r\n                    console.log(num);\r\n                  }}\r\n                >\r\n                  edit\r\n                </button>\r\n\r\n                <button\r\n                  variant=\"success\"\r\n                  onClick={() => {\r\n                    completeTask(tasks[i]._id, tasks[i].completed);\r\n                  }}\r\n                >\r\n                  complete\r\n                </button>\r\n                <button\r\n                  variant=\"danger\"\r\n                  onClick={() => {\r\n                    setNum(i);\r\n                    setDeleteModal(true);\r\n                    console.log(deleteModal + \" delete button\");\r\n                  }}\r\n                >\r\n                  delete\r\n                </button>\r\n              </div>{\" \"}\r\n            </div>\r\n          );\r\n        })}{\" \"}\r\n        {editModal && (\r\n          <div className=\"ListBox edit\">\r\n            <EditModal\r\n              taskId={tasks[num]._id}\r\n              title={tasks[num].title}\r\n              content={tasks[num].content}\r\n              onClickCancle={editCancle}\r\n            ></EditModal>\r\n          </div>\r\n        )}\r\n      </div>\r\n\r\n      {deleteModal && (\r\n        <Modal show={deleteModal} onHide={deleteCancle}>\r\n          <DeleteModal\r\n            msg={tasks[num].title}\r\n            onClickCancle={deleteCancle}\r\n            onClickConfirm={() => {\r\n              deleteConfirm(tasks[num]._id);\r\n            }}\r\n          ></DeleteModal>\r\n        </Modal>\r\n      )}\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default Tasks;\r\n","/mnt/c/Users/chaen/Desktop/김책은/web 공부/TodoList/todoapp/myreact/src/components/home.js",[],[],"/mnt/c/Users/chaen/Desktop/김책은/web 공부/TodoList/todoapp/myreact/src/components/deleteModal.js",[],[],"/mnt/c/Users/chaen/Desktop/김책은/web 공부/TodoList/todoapp/myreact/src/components/editModal.js",["73"],[],"import React, { useEffect, useState } from \"react\";\r\nimport Modal from \"react-bootstrap/Modal\";\r\n\r\nfunction EditModal({ taskId, title, content, onClickCancle }) {\r\n  const [inputTitle, setInputTitle] = useState(title);\r\n  const [inputContent, setInputContent] = useState(content);\r\n\r\n  const handleTitle = (e) => {\r\n    setInputTitle(e.target.value);\r\n  };\r\n  const handleContent = (e) => {\r\n    setInputContent(e.target.value);\r\n  };\r\n\r\n  return (\r\n    <Modal.Dialog>\r\n      <div className=\"contaianer edit\">\r\n        <form action=\"/api/edit\" method=\"POST\">\r\n          <input\r\n            type=\"text\"\r\n            value={taskId}\r\n            style={{ display: \"none\" }}\r\n            name=\"taskId\"\r\n          />\r\n          <label>title</label>\r\n          <input\r\n            type=\"text\"\r\n            className=\"form-control\"\r\n            name=\"title\"\r\n            value={inputTitle}\r\n            onChange={handleTitle}\r\n          />\r\n          <label>content</label>\r\n          <input\r\n            type=\"text\"\r\n            className=\"form-control\"\r\n            name=\"content\"\r\n            value={inputContent}\r\n            onChange={handleContent}\r\n          />\r\n\r\n          <button onClick={onClickCancle}>Close Modal</button>\r\n          <button type=\"submit\" className=\"btn btn-outline-secondary\">\r\n            Update\r\n          </button>\r\n        </form>\r\n      </div>\r\n    </Modal.Dialog>\r\n  );\r\n}\r\n\r\nexport default EditModal;\r\n",{"ruleId":"74","severity":1,"message":"75","line":6,"column":17,"nodeType":"76","messageId":"77","endLine":6,"endColumn":26},{"ruleId":"74","severity":1,"message":"78","line":7,"column":8,"nodeType":"76","messageId":"77","endLine":7,"endColumn":13},{"ruleId":"74","severity":1,"message":"75","line":1,"column":10,"nodeType":"76","messageId":"77","endLine":1,"endColumn":19},{"ruleId":"74","severity":1,"message":"79","line":1,"column":21,"nodeType":"76","messageId":"77","endLine":1,"endColumn":29},{"ruleId":"80","severity":1,"message":"81","line":10,"column":6,"nodeType":"82","endLine":10,"endColumn":8,"suggestions":"83"},{"ruleId":"84","severity":1,"message":"85","line":28,"column":7,"nodeType":"86","endLine":34,"endColumn":8},{"ruleId":"74","severity":1,"message":"87","line":3,"column":16,"nodeType":"76","messageId":"77","endLine":3,"endColumn":24},{"ruleId":"74","severity":1,"message":"88","line":5,"column":17,"nodeType":"76","messageId":"77","endLine":5,"endColumn":20},{"ruleId":"80","severity":1,"message":"89","line":17,"column":6,"nodeType":"82","endLine":17,"endColumn":8,"suggestions":"90"},{"ruleId":"74","severity":1,"message":"91","line":52,"column":9,"nodeType":"76","messageId":"77","endLine":52,"endColumn":16},{"ruleId":"74","severity":1,"message":"92","line":74,"column":9,"nodeType":"76","messageId":"77","endLine":74,"endColumn":20},{"ruleId":"93","severity":1,"message":"94","line":78,"column":3,"nodeType":"95","messageId":"96","endLine":89,"endColumn":4},{"ruleId":"74","severity":1,"message":"75","line":1,"column":17,"nodeType":"76","messageId":"77","endLine":1,"endColumn":26},"no-unused-vars","'useEffect' is defined but never used.","Identifier","unusedVar","'axios' is defined but never used.","'useState' is defined but never used.","react-hooks/exhaustive-deps","React Hook useEffe